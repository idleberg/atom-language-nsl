{
  "name": "language-nsl",
  "version": "4.0.1",
  "description": "Atom language support for nsL Assembler, including grammar, snippets and build system",
  "license": "MIT OR GPL-2.0",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode none --watch",
    "lint": "gulp lint",
    "test": "npm run lint"
  },
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "nsl assembler",
    "nsis",
    "nullsoft",
    "installer",
    "setup"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-language-nsl"
  },
  "main": "lib/main",
  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "package-deps": [
    "language-nsis"
  ],
  "activationCommands": {
    "atom-workspace": [
      "nsl-assembler:save-&-transpile"
    ]
  },
  "activationHooks": [
    "language-nsl:grammar-used"
  ],
  "consumedServices": {
    "console-panel": {
      "versions": {
        "^1.0.0": "consumeConsolePanel"
      }
    }
  },
  "dependencies": {
    "atom-package-deps": "^5.1.0",
    "atom-read-manifest": "^0.3.1",
    "atom-satisfy-dependencies": "^0.2.1"
  },
  "devDependencies": {
    "coffee-loader": "^0.9.0",
    "coffeelint-stylish": "^0.1.2",
    "coffeescript": "^2.4.1",
    "gulp": "^4.0.2",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^4.0.0",
    "gulp-jsonlint": "^1.3.1",
    "husky": "^3.0.9",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  }
}
